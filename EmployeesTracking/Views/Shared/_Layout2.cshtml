<body class="main-body app sidebar-mini ltr">

    <!-- Loader -->
    <div id="global-loader">
        <img src="/template/Html/valex/assets/img/loader.svg" class="loader-img" alt="Loader">
    </div>
    <!-- /Loader -->
    <!-- Page -->
    <div class="page custom-index">
        <div>
            <!-- main-header -->
            @await Html.PartialAsync("_MainHeaderPartial")
            <!-- /main-header -->
            <!-- main-sidebar -->
            @await Html.PartialAsync("_MainSidebar")
            <!-- main-sidebar -->
        </div>

        <!-- main-content -->
        @RenderBody()
        <!-- /main-content -->
        <!-- Sidebar-right-->
        @await Html.PartialAsync("_SidebarRight")
        <!--/Sidebar-right-->
        <!-- Message Modal -->
        @await Html.PartialAsync("_MessageModalPartial")

        <!--Video Modal -->
        @await Html.PartialAsync("_VideoModalPartial")
        <!-- modal -->
        <!-- Audio Modal -->
        @await Html.PartialAsync("_AudioModalPartial")
        <!-- modal -->
        <!-- Footer opened -->
        @await Html.PartialAsync("_FooterPartial")
        <!-- Footer closed -->

    </div>
    <!-- End Page -->
    <!-- Back-to-top -->
    <a href="#top" id="back-to-top"><i class="las la-angle-double-up"></i></a>

